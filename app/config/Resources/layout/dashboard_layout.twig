{% extends "default_layout.twig" %}
{% block body %}
  <div id="app">
    {% block header %}
      <!-- ========== HEADER ========== -->
      <header class="u-header u-header--fix-top">
        <!-- Breadcrumb Section -->
        <div class="bg-primary">
          <div class="container-fluid">
            <div class="row">
              <div class="col-lg-5 order-lg-2 text-lg-right my-lg-4">
                <div class="d-flex d-lg-inline-block justify-content-between justify-content-lg-end align-items-center align-items-lg-start">
                  <!-- Breadcrumb -->
                  <ol class="breadcrumb breadcrumb-white breadcrumb-no-gutter mb-0">
                    <a class="d-none d-lg-flex navbar-brand u-header__navbar-brand-center u-header__navbar-brand-text-white" href="/dashboard" aria-label="Front">
                      <img src="{{assets}}/img/logo_eatery.png" width="65">
                      <span class="u-header__navbar-brand-text">Eatery</span>
                    </a>
                    <!-- Default Logo -->
                    <a class="d-lg-none navbar-brand u-header__navbar-brand-center u-header__navbar-brand-collapsed" href="/dashboard" aria-label="Front">
                      <img src="{{assets}}/img/logo_eatery.png" width="50">
                      <span class="d-inline-block u-header__navbar-brand-text text-white">Eatery</span>
                    </a>
                    <!-- End Default Logo -->

                    <div class="d-lg-flex justify-content-lg-between align-items-lg-center d-none">
                      <div class="ml-lg-auto">
                        <!-- Button -->
                        <a class="btn btn-sm btn-soft-white text-nowrap transition-3d-hover" id="sidebarNavToggler" href="javascript:;" role="button" aria-expanded="false" aria-haspopup="true"
                          data-unfold-animation-in="fadeInRight"
                          data-unfold-animation-out="fadeOutRight"
                          data-unfold-duration="500"
                          data-unfold-event="click"
                          data-unfold-hide-on-scroll="false"
                          data-unfold-target="#sidebarContent"
                          data-unfold-type="css-animation">
                          <span class="fas fa-cog mr-2"></span>
                          Configuracion
                        </a>
                        <!-- End Button -->
                      </div>
                    </div>
                  </ol>
                  <!-- End Breadcrumb -->

                  <!-- Breadcrumb Nav Toggle Button -->
                  <div class="d-lg-none">
                    <button class="navbar-toggler btn u-hamburger u-hamburger--white" id="sidebarNavToggler" type="button" aria-controls="sidebarContent" aria-expanded="false" aria-haspopup="true" aria-label="Toggle navigation"
                      data-toggle="collapse"
                      data-unfold-animation-in="fadeInRight"
                      data-unfold-animation-out="fadeOutRight"
                      data-unfold-duration="500"
                      data-unfold-event="click"
                      data-unfold-hide-on-scroll="false"
                      data-unfold-target="#sidebarContent"
                      data-unfold-type="css-animation">
                      <span class="u-hamburger__box" id="breadcrumbHamburgerTrigger">
                        <span class="u-hamburger__inner"></span>
                      </span>
                    </button>
                  </div>
                  <!-- End Breadcrumb Nav Toggle Button -->
                </div>
              </div>

              <div class="col-lg-7 order-lg-1 d-none d-lg-flex my-lg-4"
                v-if="User">
                <a href="/dashboard">
                  <!-- User Info -->
                  <div class="media d-block d-sm-flex align-items-sm-center">
                    <div class="u-lg-avatar position-relative mb-3 mb-sm-0 mr-3">
                      <img class="img-fluid rounded-circle" alt="Image Description" :src="User.photoURL">
                      <span class="badge badge-md badge-outline-success badge-pos badge-pos--bottom-right rounded-circle"
                        v-if="User.emailVerified">
                        <span class="fas fa-check"></span>
                      </span>
                    </div>
                    <div class="media-body">
                      <h1 class="h3 text-white font-weight-medium mb-1">([User.displayName])</h1>
                      <span class="d-block text-white">([User.email])</span>
                    </div>
                  </div>
                  <!-- End User Info -->
                </a>
              </div>
            </div>
          </div>
        </div>
        <!-- End Breadcrumb Section -->
      </header>
      <!-- ========== END HEADER ========== -->
    {% endblock %}

    <!-- Account Sidebar Navigation -->
    <aside class="u-sidebar" id="sidebarContent" aria-labelledby="sidebarNavToggler">
      <div class="u-sidebar__scroller">
        <div class="u-sidebar__container">
          <div class="u-sidebar--account__footer-offset">
            <!-- Toggle Button -->
            <div class="d-flex justify-content-between align-items-center pt-4 px-7">
              <h3 class="h6 mb-0">Mi cuenta</h3>

              <button class="close ml-auto" type="button" aria-controls="sidebarContent" aria-expanded="false" aria-haspopup="true"
                data-unfold-animation-in="fadeInRight"
                data-unfold-animation-out="fadeOutRight"
                data-unfold-duration="500"
                data-unfold-event="click"
                data-unfold-hide-on-scroll="false"
                data-unfold-target="#sidebarContent"
                data-unfold-type="css-animation">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <!-- End Toggle Button -->

            <!-- Content -->
            <div class="js-scrollbar u-sidebar__body">
              <!-- Holder Info -->
              <header class="d-flex align-items-center u-sidebar--account__holder mt-3"
                v-if="User">
                <div class="position-relative">
                  <img class="u-sidebar--account__holder-img" alt="Image Description" :src="User.photoURL">
                  <span class="badge badge-xs badge-outline-success badge-pos rounded-circle"></span>
                </div>
                <div class="ml-3">
                  <span class="font-weight-semi-bold">([User.displayName])
                    <span class="badge badge-success ml-1">Administrador</span>
                  </span>
                  <span class="u-sidebar--account__holder-text">([User.email])</span>
                </div>

                <!-- Settings -->
                <div class="btn-group position-relative ml-auto mb-auto">
                  <a class="btn btn-xs btn-icon btn-text-secondary rounded" id="sidebar-account-settings-invoker" href="javascript:;" role="button" aria-controls="sidebar-account-settings" aria-expanded="false" aria-haspopup="true"
                    data-toggle="dropdown"
                    data-unfold-animation-in="slideInUp"
                    data-unfold-animation-out="fadeOut"
                    data-unfold-delay="300"
                    data-unfold-duration="300"
                    data-unfold-event="click"
                    data-unfold-target="#sidebar-account-settings"
                    data-unfold-type="css-animation">
                    <span class="fas fa-ellipsis-v btn-icon__inner"></span>
                  </a>
                </div>
                <!-- End Settings -->
              </header>
              <!-- End Holder Info -->
              <div class="u-sidebar__content--account">
                <!-- List Links -->
                <ul class="list-unstyled u-sidebar--account__list">
                  <li class="u-sidebar--account__list-item">
                    <a class="u-sidebar--account__list-link" href="/dashboard">
                      <span class="fas fa-home u-sidebar--account__list-icon mr-2"></span>
                      Inicio
                    </a>
                  </li>
                  <li class="u-sidebar--account__list-item">
                    <a class="u-sidebar--account__list-link" href="/dashboard/profile">
                      <span class="fas fa-user-circle u-sidebar--account__list-icon mr-2"></span>
                      Perfil
                    </a>
                  </li>
                  <li class="u-sidebar--account__list-item">
                    <a class="u-sidebar--account__list-link" href="javascript:void(0)" @click="signOut">
                      <span class="fas fa-window-close u-sidebar--account__list-icon mr-2"></span>
                      Cerrar Sesion
                    </a>
                  </li>
                </ul>
                <!-- End List Links -->

                <div class="u-sidebar--account__list-divider"></div>
              </div>
            </div>
          </div>

          <!-- Footer -->
          <footer class="svg-preloader u-sidebar__footer u-sidebar__footer--account" id="SVGwaveWithDots">
            <ul class="list-inline mb-0">
              <li class="list-inline-item pr-3">
                <a class="u-sidebar__footer--account__text" href="#">Privacy</a>
              </li>
              <li class="list-inline-item pr-3">
                <a class="u-sidebar__footer--account__text" href="#">Terms</a>
              </li>
              <li class="list-inline-item">
                <a class="u-sidebar__footer--account__text" href="#">
                  <i class="fas fa-info-circle"></i>
                </a>
              </li>
            </ul>

            <!-- SVG Background Shape -->
            <div class="position-absolute right-0 bottom-0 left-0">
              <img class="js-svg-injector" src="{{assets}}/svg/components/wave-bottom-with-dots.svg" alt="Image Description"
                data-parent="#SVGwaveWithDots">
            </div>
            <!-- End SVG Background Shape -->
          </footer>
          <!-- End Footer -->
        </div>
      </div>
    </aside>
    <!-- End Account Sidebar Navigation -->
    <!-- Alerts -->
    <div class="alert-fixed-bottom"
      v-show="alerts.length > 0">
      <div role="alert"
        v-for="(alert, index) in alerts" :class="'alert '+alert.type+'  alert-dismissible animated fadeInUp delay-3 my-3 mx-10'">
        <span
          v-html="alert.msj"></span>
        <button class="close" type="button" aria-label="Close"
          data-dismiss="alert" @click="alerts.splice(index, 1)">
          <span aria-hidden="true">×</span>
        </button>
      </div>
    </div>
    <!-- End Alert -->
  </div>
  {% block container %}{% endblock %}
  <!-- Dishes Modal Window -->
  <div class="modal fade" id="alertDeleteModal" role="dialog" tabindex="-1" aria-hidden="true" aria-labelledby="alertDeleteModalLabel">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header bg-danger">
          <h5 class="modal-title text-white" id="alertDeleteModalLabel">Aviso de Eliminacion</h5>
          <button class="close" type="button" aria-label="Close"
            data-dismiss="modal" @click="Dish = []">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div class="card-body p-5">
            <div class="row">
              <h6>¿Esta seguro que desea eliminar este registro?</h6>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button class="btn btn-sm btn-soft-secondary transition-3d-hover mr-1" type="button"
            data-dismiss="modal" @click="Dish = []">Cancelar</button>
          <button class="btn btn-sm btn-danger transition-3d-hover" id="deleted" type="button">
            <i class="fas fa-trash"></i>
            Eliminar</button>
        </div>
      </div>
    </div>
  </div>
  <!-- End Dishes Modal Window -->
  {% block footer %}
    <!-- ========== FOOTER ========== -->
    <footer>
      <!-- Lists -->
      <div class="border-bottom">
        <div class="container space-2">
          <div class="row justify-content-md-between">
            <div class="col-sm-4 col-lg-2 mb-4 mb-lg-0">
              <h4 class="h6 font-weight-semi-bold">About</h4>

              <!-- List Group -->
              <ul class="list-group list-group-flush list-group-borderless mb-0">
                <li>
                  <a class="list-group-item list-group-item-action" href="../pages/about-agency.html">About</a>
                </li>
                <li>
                  <a class="list-group-item list-group-item-action" href="../pages/services-agency.html">Services</a>
                </li>
                <li>
                  <a class="list-group-item list-group-item-action" href="../pages/careers.html">Careers</a>
                </li>
              </ul>
              <!-- End List Group -->
            </div>

            <div class="col-sm-4 col-lg-2 mb-4 mb-lg-0">
              <h4 class="h6 font-weight-semi-bold">Account</h4>

              <!-- List Group -->
              <ul class="list-group list-group-flush list-group-borderless mb-0">
                <li>
                  <a class="list-group-item list-group-item-action" href="dashboard.html">Account</a>
                </li>
                <li>
                  <a class="list-group-item list-group-item-action" href="my-tasks.html">My tasks</a>
                </li>
                <li>
                  <a class="list-group-item list-group-item-action" href="projects.html">Projects</a>
                </li>
              </ul>
              <!-- End List Group -->
            </div>

            <div class="col-sm-4 col-lg-2 mb-4 mb-lg-0">
              <h4 class="h6 font-weight-semi-bold">Resources</h4>

              <!-- List Group -->
              <ul class="list-group list-group-flush list-group-borderless mb-0">
                <li>
                  <a class="list-group-item list-group-item-action" href="../pages/help.html">Help</a>
                </li>
                <li>
                  <a class="list-group-item list-group-item-action" href="../pages/terms.html">Terms</a>
                </li>
                <li>
                  <a class="list-group-item list-group-item-action" href="../pages/privacy.html">Privacy</a>
                </li>
              </ul>
              <!-- End List Group -->
            </div>

            <div class="col-md-6 col-lg-4">
              <h4 class="h6 font-weight-semi-bold mb-4">We are driven to deliver results for all your businesses.</h4>

              <!-- Button -->
              <button class="btn btn-xs btn-dark btn-wide transition-3d-hover text-left mb-2 mr-1" type="button">
                <span class="media align-items-center">
                  <span class="fab fa-apple fa-2x mr-3"></span>
                  <span class="media-body">
                    <span class="d-block">Download on the</span>
                    <strong class="font-size-1">App Store</strong>
                  </span>
                </span>
              </button>
              <!-- End Button -->

              <!-- Button -->
              <button class="btn btn-xs btn-dark btn-wide transition-3d-hover text-left mb-2" type="button">
                <span class="media align-items-center">
                  <span class="fab fa-google-play fa-2x mr-3"></span>
                  <span class="media-body">
                    <span class="d-block">Get it on</span>
                    <strong class="font-size-1">Google Play</strong>
                  </span>
                </span>
              </button>
              <!-- End Button -->
            </div>
          </div>
        </div>
      </div>
      <!-- End Lists -->

      <!-- Copyright -->
      <div class="container text-center space-1">
        <!-- Logo -->
        <a class="d-inline-flex align-items-center mb-2" href="../home/index.html" aria-label="Front">
          <svg style="margin-bottom: 0;" height="36px" viewbox="0 0 46 46" width="36px" x="0px" xml:space="preserve" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://www.w3.org/2000/svg" y="0px">
            <path d="M23,41L23,41c-9.9,0-18-8-18-18v0c0-9.9,8-18,18-18h11.3C38,5,41,8,41,11.7V23C41,32.9,32.9,41,23,41z" fill="#3F7DE0" opacity=".65"></path>
            <path class="fill-info" d="M28,35.9L28,35.9c-9.9,0-18-8-18-18v0c0-9.9,8-18,18-18l11.3,0C43,0,46,3,46,6.6V18C46,27.9,38,35.9,28,35.9z" opacity=".5"></path>
            <path class="fill-primary" d="M18,46L18,46C8,46,0,38,0,28v0c0-9.9,8-18,18-18h11.3c3.7,0,6.6,3,6.6,6.6V28C35.9,38,27.9,46,18,46z" opacity=".7"></path>
            <path class="fill-white" d="M17.4,34V18.3h10.2v2.9h-6.4v3.4h4.8v2.9h-4.8V34H17.4z"></path>
          </svg>
          <span class="brand brand-primary">Front</span>
        </a>
        <!-- End Logo -->
        <p class="small text-muted">&copy; Front. 2019 Htmlstream. All rights reserved.</p>
      </div>
      <!-- End Copyright -->
    </footer>
    <!-- ========== END FOOTER ========== -->
  {% endblock %}
  <!-- Go to Top -->
  <a class="js-go-to u-go-to" href="#"
    data-compensation="#header"
    data-hide-effect="slideOutDown"
    data-offset-top="400"
    data-position='{"bottom": 15, "right": 15 }'
    data-show-effect="slideInUp"
    data-type="fixed">
    <span class="fas fa-arrow-up u-go-to__inner"></span>
  </a>
  <!-- End Go to Top -->
{% endblock %}

{% block jsPlugins %}
  <script src="{{assets}}/vendor/hs-megamenu/src/hs.megamenu.js"></script>
  <script src="{{assets}}/vendor/svg-injector/dist/svg-injector.min.js"></script>
  <script src="{{assets}}/vendor/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>
  <script src="{{assets}}/vendor/jquery-validation/dist/jquery.validate.min.js"></script>
  <script src="{{assets}}/vendor/custombox/dist/custombox.min.js"></script>
  <script src="{{assets}}/vendor/custombox/dist/custombox.legacy.min.js"></script>
  <script src="{{assets}}/vendor/flatpickr/dist/flatpickr.min.js"></script>
  <script src="{{assets}}/vendor/appear.js"></script>
  <script src="{{assets}}/vendor/circles/circles.min.js"></script>
  <script src="{{assets}}/vendor/fancybox/jquery.fancybox.min.js"></script>
  {% block addJsPlugins %}{% endblock %}
{% endblock %}

