 <!-- Tables Modal Window -->
<div id="qrCodeTableModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="qrCodeTableModalLabel" aria-hidden="true">
	<div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="qrCodeTableModalLabel">Codigo QR Mesa</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close" @click="Table = []">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
			<div class="modal-body bg-primary">
					<div class="card-body p-5">
						<div class="row">
							<div class="col text-center">
								<img id="qrCode" class="img-fluid border border-primary rounded-lg" width="250">
							</div>
						</div>
					</div>
			</div>
			<!-- Buttons -->
			<div class="modal-footer">
				<button type="button" class="btn btn-sm btn-soft-secondary transition-3d-hover mr-1" data-dismiss="modal"  @click="Table = [];file = '';">Cancelar</button>
				<a download="tableQRCodePNG" :href="qrCodeURL" class="btn btn-sm btn-info transition-3d-hover"><i class="fas fa-download"></i> Descargar</a>
			</div>
			<!-- End Buttons -->
		</div>
	</div>
</div>
<!-- End Tables Modal Window -->